{
  "name": "ninecodes-social-manager",
  "title": "Social Media Manager",
  "version": "2.0.0-alpha.2",
  "wordpress": {
    "requires_at_least": "4.7",
    "tested_up_to": "4.7.3"
  },
  "description": "Lightweight, clean and optimized social media plugin for WordPress.",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "precommit": "dev-lib/pre-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninecodes/social-manager.git"
  },
  "keywords": [
    "widget",
    "json",
    "wp-api",
    "social-media",
    "sharing",
    "facebook",
    "twitter",
    "pinterest",
    "open-graph",
    "twitter-cards"
  ],
  "author": {
    "name": "NineCodes",
    "email": "admin@ninecodes.com",
    "url": "https://github.com/ninecodes"
  },
  "contributors": [
    "Thoriq Firdaus <tfirdaus@outlook.com>",
    "Hongkiat Lim <me@hongkiat.com>"
  ],
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/ninecodes/social-manager/issues"
  },
  "homepage": "http://wordpress.org/plugins/ninecodes-social-manager",
  "devDependencies": {
    "eslint": "^3.9.1",
    "grunt": "~1.0.1",
    "grunt-checktextdomain": "~1.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.3.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-cssmin": "~2.0.0",
    "grunt-contrib-qunit": "^2.0.0",
    "grunt-contrib-uglify": "^2.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-rtlcss": "^2.0.1",
    "grunt-shell": "~2.1.0",
    "grunt-string-replace": "^1.3.1",
    "grunt-wp-i18n": "^1.0.0",
    "husky": "^0.13.3",
    "qunitjs": "^2.3.1",
    "validate-commit-msg": "^2.12.1"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 50,
      "helpMessage": "\nPlease fix your commit message to conform with Commitizen standard.\n",
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build",
        "release"
      ],
      "warnOnFail": false,
      "autoFix": false
    }
  }
}
